{
  "name": "api.andyspotting.com",
  "version": "1.0.0",
  "description": "API for andyspotting.com",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon dist/index.js",
    "build": "babel src -d dist",
    "watch": "babel src -d dist --watch",
    "fix": "npm run format && npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,json,scss}\"",
    "lint": "eslint src",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{json,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aj-may/api.andyspotting.com.git"
  },
  "author": "A.J. May",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aj-may/api.andyspotting.com/issues"
  },
  "homepage": "https://github.com/aj-may/api.andyspotting.com#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "@google-cloud/datastore": "^1.4.2",
    "@google-cloud/storage": "^1.7.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "exif-parser": "^0.1.12",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "multer": "^1.3.1",
    "nodemon": "^1.18.4",
    "statuses": "^1.5.0",
    "thumbor-url-builder": "^0.1.1",
    "uuid": "^3.3.2"
  }
}
